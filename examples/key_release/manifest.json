{
    "testName": "key_release",
    "images": {},
    "containerGroups": [
        {
            "containers": [
                {
                    "image": "http://mcr.microsoft.com/aci/skr:2.2",
                    "command": [ "/skr.sh" ],
                    "ports": [ 8080 ],
                    "cores": 1,
                    "ram": 2
                },
                {
                    "image": "http://mcr.microsoft.com/aci/skr:2.2",
                    "command": [ "/tests/skr/skr_client.sh" ],
                    "env": {
                        "SkrClientMAAEndpoint": "$AZURE_ATTESTATION_ENDPOINT",
                        "SkrClientAKVEndpoint": "$AZURE_HSM_ENDPOINT",
                        "SkrClientKID": "[concat(variables('uniqueId'), '-key')]"
                    },
                    "cores": 1,
                    "ram": 2,
                    "ports": []
                },
                {
                    "image": "http://mcr.microsoft.com/aci/skr:2.2",
                    "command": [ "/tests/skr/attest_client.sh" ],
                    "env": {
                        "AttestClientRuntimeData": ""
                    },
                    "cores": 1,
                    "ram": 2,
                    "ports": []
                },
                {
                    "image": "http://mcr.microsoft.com/aci/skr:2.2",
                    "command": [ "/tests/skr/attest_client.sh" ],
                    "env": {
                        "AttestClientRuntimeData": "",
                        "AttestClientMAAEndpoint": "$AZURE_ATTESTATION_ENDPOINT"
                    },
                    "cores": 1,
                    "ram": 2,
                    "ports": []
                }
            ],
            "ports": [ 8080 ]
        }
    ],
    "securityPolicies": [],
    "registryCredentials": {},
    "preDeployScript": "deploy_key.py"
}