{
    "testName": "simple_sidecar",
    "images": {
        "primary": "primary.Dockerfile",
        "sidecar": "sidecar.Dockerfile"
    },
    "containerGroups": [
        {
            "containers": [
                {
                    "image": "primary",
                    "ports": [8000],
                    "cores": 2,
                    "ram": 8
